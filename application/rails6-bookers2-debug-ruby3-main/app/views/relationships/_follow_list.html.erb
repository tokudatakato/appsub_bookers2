<h2>Follower Users</h2>
<% if users.exists? %>
  <% users.each do |user| %>
    <table>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td></td>
          <td><%= user.name %></td>
          <td>フォロー数: <%= user.follower.count %></td>
          <td>フォロワー数: <%= user.followed.count %></td>
          <td><%= link_to "Show", user_path(user) %></td>
        </tr>
      </tbody>
    </table>
  <% end %>
<% else %>
  <p>ユーザーはいません</p>
<% end %>